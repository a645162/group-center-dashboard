{
  "name": "group-center-dashboard",
  "version": "1.5.3",
  "private": true,
  "author": "Haomin Kong <a645162@qq.com>",
  "scripts": {
    "build": "max build",
    "check:compile": "tsc --noEmit",
    "dev": "max dev",
    "dev:proxy": "cross-env ENABLE_PROXY=true MOCK=none max dev",
    "dev:pure": "cross-env MOCK=none max dev",
    "format": "prettier --cache --write .",
    "postinstall": "max setup",
    "lint": "max lint",
    "lint:style": "bun run stylelint \"src/**/*.less\"",
    "mirrors": "nrm use taobao",
    "openapi": "cross-env ENABLE_OPEN_API=true max openapi",
    "openapi:old": "ts-node openapi.config.ts",
    "openapi:win": "set ENABLE_OPEN_API=true && max openapi",
    "prepare": "husky",
    "setup": "max setup",
    "start": "npm run dev",
    "stat": "tokei",
    "tree": "dep-tree entropy src/*",
    "update": "ncu -u"
  },
  "dependencies": {
    "@ant-design/charts": "^2.6.6",
    "@ant-design/colors": "^8.0.0",
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.10",
    "@szhsin/react-menu": "^4.5.0",
    "@umijs/max": "^4.5.3",
    "antd": "^5.27.6",
    "antd-style": "^3.7.1",
    "axios": "^1.13.1",
    "es-toolkit": "^1.41.0",
    "react-live-clock": "6.1.25",
    "react-number-format": "^5.4.4",
    "typed.js": "^2.1.0",
    "typescript-cookie": "^1.0.6",
    "ua-parser-js": "^2.0.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@dep-tree/cli": "^0.23.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@umijs/max-plugin-openapi": "^2.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "npm-check-updates": "^19.1.2",
    "nrm": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "tokei": "^0.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
