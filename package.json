{
  "name": "group-center-dashboard",
  "private": true,
  "author": "Haomin Kong <a645162@qq.com>",
  "scripts": {
    "build": "max build",
    "check": "eslint",
    "dev": "max dev",
    "dev-proxy": "cross-env ENABLE_PROXY=true MOCK=none max dev",
    "dev-pure": "cross-env MOCK=none max dev",
    "format": "prettier --cache --write .",
    "postinstall": "max setup",
    "mirrors": "nrm use taobao",
    "openapi": "cross-env ENABLE_OPEN_API=true max openapi",
    "openapi-old": "ts-node openapi.config.ts",
    "openapi-win": "set ENABLE_OPEN_API=true && max openapi",
    "prepare": "husky",
    "setup": "max setup",
    "start": "npm run dev",
    "stat": "tokei",
    "tree": "dep-tree entropy src/*",
    "update": "ncu -u"
  },
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "@ant-design/icons": "^5.5.2",
    "@ant-design/pro-components": "^2.8.2",
    "@szhsin/react-menu": "^4.2.3",
    "@umijs/max": "^4.3.36",
    "antd": "^5.22.3",
    "antd-style": "^3.7.1",
    "axios": "^1.7.9",
    "es-toolkit": "^1.29.0",
    "react-live-clock": "6.1.25",
    "react-number-format": "^5.4.2",
    "typed.js": "^2.1.0",
    "typescript-cookie": "^1.0.6",
    "ua-parser-js": "^2.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@dep-tree/cli": "^0.23.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@umijs/max-plugin-openapi": "^2.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.11",
    "nrm": "^1.5.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "tailwindcss": "^3.4.16",
    "tokei": "^0.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
