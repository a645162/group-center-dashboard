{
  "name": "group-center-dashboard",
  "private": true,
  "author": "Haomin Kong <a645162@qq.com>",
  "scripts": {
    "build": "max build",
    "check": "eslint",
    "dev": "max dev",
    "dev:proxy": "cross-env ENABLE_PROXY=true MOCK=none max dev",
    "dev:pure": "cross-env MOCK=none max dev",
    "format": "prettier --cache --write .",
    "postinstall": "max setup",
    "mirrors": "nrm use taobao",
    "openapi": "cross-env ENABLE_OPEN_API=true max openapi",
    "openapi:old": "ts-node openapi.config.ts",
    "openapi:win": "set ENABLE_OPEN_API=true && max openapi",
    "prepare": "husky",
    "setup": "max setup",
    "start": "npm run dev",
    "stat": "tokei",
    "tree": "dep-tree entropy src/*",
    "update": "ncu -u"
  },
  "dependencies": {
    "@ant-design/colors": "^8.0.0",
    "@ant-design/icons": "^6.0.0",
    "@ant-design/pro-components": "^2.8.9",
    "@szhsin/react-menu": "^4.4.1",
    "@umijs/max": "^4.4.11",
    "antd": "^5.26.5",
    "antd-style": "^3.7.1",
    "axios": "^1.10.0",
    "es-toolkit": "^1.39.7",
    "react-live-clock": "6.1.25",
    "react-number-format": "^5.4.4",
    "typed.js": "^2.1.0",
    "typescript-cookie": "^1.0.6",
    "ua-parser-js": "^2.0.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@dep-tree/cli": "^0.23.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@umijs/max-plugin-openapi": "^2.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-check-updates": "^18.0.1",
    "nrm": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.18",
    "tailwindcss": "^3.4.17",
    "tokei": "^0.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
